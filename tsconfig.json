{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "outDir": "docs",
    "rootDir": ".",
    "baseUrl": "./src",
    "typeRoots": ["node_modules/@types"],
    "target": "esnext",
  }
}
